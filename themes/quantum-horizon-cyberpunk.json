    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#4D5B82",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "string constant.other.placeholder"],
            "settings": {
                "foreground": "#00F6FF"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#FF2E88",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "meta.function-call"],
            "settings": {
                "foreground": "#3FFF9E"
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.type.class", "entity.name.class"],
            "settings": {
                "foreground": "#FFE600",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#FF65A3"
            }
        },
        {
            "name": "Decorators",
            "scope": ["meta.decorator", "meta.decorator.identifier"],
            "settings": {
                "foreground": "#FFE600",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Properties",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#00F6FF"
            }
        },
        {
            "name": "CSS Values",
            "scope": "support.constant.property-value.css",
            "settings": {
                "foreground": "#FF65A3"
            }
        },
        {
            "name": "JSON Keys",
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#00F6FF"
            }
        },
        {
            "name": "YAML Keys",
            "scope": "entity.name.tag.yaml",
            "settings": {
                "foreground": "#FF2E88"
            }
        }
    ],
    "semanticTokenColors": {
        "namespace": "#FF2E88",
        "type": "#FFE600",
        "class": "#FFE600",
        "enum": "#FFE600",
        "interface": "#00F6FF",
        "struct": "#FFE600",
        "typeParameter": "#FF65A3",
        "parameter": "#00F6FF",
        "variable": "#00F6FF",
        "property": "#3FFF9E",
        "function": "#3FFF9E",
        "method": "#3FFF9E",
        "decorator": "#FFE600",
        "regex": "#FF65A3",
        "number": "#FFE600",
        "boolean": "#FF2E88",
        "string": "#FF65A3",
        "keyword": "#FF2E88",
        "operator": "#00F6FF",
        "enumMember": "#3FFF9E",
        "event": "#FFE600",
        "modifier": "#FF2E88",
        "comment": "#4D5B82",
        "macro": "#FF2E88",
        "lifetime": "#FF65A3",
        "deriving": "#00F6FF",
        "attribute": "#FFE600",
        "selfKeyword": "#FF2E88",
        "builtinType": "#00F6FF",
        "typeAlias": "#FFE600",
        "unresolvedReference": "#FF4081",
        "customLiteral": "#3FFF9E"
