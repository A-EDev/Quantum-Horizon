    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#3B4C72",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#4D9BF8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "meta.function-call"],
            "settings": {
                "foreground": "#4CD4A1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.type.class", "entity.name.class"],
            "settings": {
                "foreground": "#FFCC66",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.parameter"],
            "settings": {
                "foreground": "#D8E3F4"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#CF79FF"
            }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#FF8599"
            }
        },
        {
            "name": "Constants",
            "scope": ["constant"],
            "settings": {
                "foreground": "#73AFF9"
            }
        },

        {
            "name": "Regex",
            "scope": ["string.regexp", "constant.regexp"],
            "settings": {
                "foreground": "#FFCC66",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Escape Characters",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#CF79FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "URL",
            "scope": ["*url*", "*link*", "*uri*"],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#73AFF9"
            }
        },
        {
            "name": "Decorators",
            "scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
            "settings": {
                "foreground": "#4CD4A1",
                "fontStyle": "italic"
            }
        },
        {
            "name": "ES7 Bind Operator",
            "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
            "settings": {
                "foreground": "#FF6B80",
                "fontStyle": "italic"
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
            "settings": {
                "foreground": "#4D9BF8"
            }
        },
        {
            "name": "JSON Key - Level 1",
            "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
            "settings": {
                "foreground": "#73AFF9"
            }
        },
        {
            "name": "JSON Key - Level 2",
            "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
            "settings": {
                "foreground": "#4CD4A1"
            }
        },
        {
            "name": "Markdown - Plain",
            "scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
            "settings": {
                "foreground": "#D8E3F4"
            }
        },
        {
            "name": "Markdown - Headers",
            "scope": ["markdown.heading", "markup.heading | markup.heading entity.name", "markup.heading.markdown punctuation.definition.heading.markdown"],
            "settings": {
                "foreground": "#4D9BF8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown - Inline Code",
            "scope": ["markup.inline.raw.string.markdown", "markup.fenced_code.block.markdown"],
            "settings": {
                "foreground": "#4CD4A1"
            }
        }
    ],
    "semanticTokenColors": {
        "namespace": "#4D9BF8",
        "type": "#FFCC66",
        "class": "#FFCC66",
        "enum": "#4D9BF8",
        "interface": "#73AFF9",
