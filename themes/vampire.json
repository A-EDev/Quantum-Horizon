        "selection.background": "#FF336655"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#8B6670",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#FFB3B3"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
            "settings": {
                "foreground": "#FF4778"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#B088FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "meta.function-call", "support.function"],
            "settings": {
                "foreground": "#FF3366",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.type", "entity.name.class", "entity.name.namespace"],
            "settings": {
                "foreground": "#FF6699",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.parameter"],
            "settings": {
                "foreground": "#E6D9DD"
            }
        },
        {
            "name": "Tags",
            "scope": ["entity.name.tag", "punctuation.definition.tag"],
            "settings": {
                "foreground": "#FF3366"
            }
        },
        {
            "name": "Decorators",
            "scope": ["meta.decorator", "meta.decorator entity.name.function"],
            "settings": {
                "foreground": "#B088FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Important Keywords",
            "scope": ["keyword.control.flow", "keyword.control.conditional"],
            "settings": {
                "foreground": "#FF4778",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Special Variables",
            "scope": ["variable.language.this", "variable.language.super", "variable.language.self"],
            "settings": {
                "foreground": "#FF6699",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Type Annotations",
            "scope": ["meta.type.annotation"],
            "settings": {
                "foreground": "#B088FF"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": ["markup.heading", "punctuation.definition.heading.markdown"],
            "settings": {
                "foreground": "#FF3366",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": ["markup.underline.link", "string.other.link.title.markdown"],
            "settings": {
                "foreground": "#B088FF",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown Code Blocks",
            "scope": ["markup.fenced_code", "markup.inline.raw"],
            "settings": {
                "foreground": "#99CC99"
            }
        },
        {
            "name": "Markdown Lists",
            "scope": ["markup.list.unnumbered", "markup.list.numbered"],
            "settings": {
                "foreground": "#FF6699"
            }
        },
        {
            "name": "Markdown Quotes",
            "scope": ["markup.quote"],
            "settings": {
                "foreground": "#FF4778",
                "fontStyle": "italic"
            }
        },
        {
            "name": "JSON Keys",
            "scope": ["support.type.property-name.json"],
            "settings": {
                "foreground": "#FF3366"
            }
        },
        {
            "name": "CSS Properties",
            "scope": ["support.type.property-name.css"],
            "settings": {
                "foreground": "#FF4778"
            }
        },
        {
            "name": "CSS Values",
            "scope": ["support.constant.property-value.css"],
            "settings": {
                "foreground": "#FF6699"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "property": "#FF6699",
        "type": "#FF4778",
        "class": "#FF3366",
        "enum": "#B088FF",
        "interface": "#99CC99",
        "typeParameter": "#FFCC66",
        "parameter": "#E6D9DD",
        "namespace": "#FF6699",
        "class.defaultLibrary": "#FF3366",
        "variable.defaultLibrary": "#FF4778",
        "function.defaultLibrary": "#FF3366",
        "operator": "#FF6699",
        "decorator": {
            "foreground": "#B088FF",
            "fontStyle": "italic bold"
        }
    }
}
