    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#424258",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"],
            "settings": {
                "foreground": "#7D5DEC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": ["entity.name.function", "meta.function-call", "meta.method-call"],
            "settings": {
                "foreground": "#50FA7B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes and Types",
            "scope": ["entity.name.type.class", "entity.name.class", "entity.name.type"],
            "settings": {
                "foreground": "#8BE9FD",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": ["variable", "variable.parameter"],
            "settings": {
                "foreground": "#E2E2E6"
            }
        },
        {
            "name": "Constants and Properties",
            "scope": ["variable.other.constant", "variable.other.property"],
            "settings": {
                "foreground": "#9590B3"
            }
        },
        {
            "name": "Strings and Templates",
            "scope": ["string", "string.template"],
            "settings": {
                "foreground": "#FF79C6"
            }
        },
        {
            "name": "Unit Tests",
            "scope": ["entity.name.function.test", "entity.name.function.test.js", "entity.name.function.test.ts"],
            "settings": {
                "foreground": "#50FA7B",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "GraphQL Operations",
            "scope": ["keyword.operation.graphql", "keyword.type.graphql"],
            "settings": {
                "foreground": "#FF79C6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "TOML Keys",
            "scope": "keyword.key.toml",
            "settings": {
                "foreground": "#8BE9FD"
            }
        },
        {
            "name": "Protocol Buffers",
            "scope": ["keyword.proto", "message.proto"],
            "settings": {
                "foreground": "#BD93F9",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Arduino Keywords",
            "scope": [
                "keyword.control.arduino",
                "support.function.arduino",
                "constant.numeric.arduino",
                "support.constant.arduino"
            ],
            "settings": {
                "foreground": "#7D5DEC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Arduino Functions",
            "scope": [
                "entity.name.function.arduino",
                "support.function.arduino"
            ],
            "settings": {
                "foreground": "#50FA7B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "C++ Keywords",
            "scope": [
                "keyword.control.c++",
                "storage.modifier.c++",
                "keyword.operator.new.c++"
            ],
            "settings": {
                "foreground": "#BD93F9",
                "fontStyle": "bold"
            }
        },
        {
            "name": "C++ Types",
            "scope": [
                "storage.type.c++",
                "storage.type.primitive.c++",
                "storage.type.template.c++"
            ],
            "settings": {
                "foreground": "#8BE9FD",
                "fontStyle": "italic"
            }
        },
        {
            "name": "C++ Macros",
            "scope": [
                "entity.name.function.preprocessor.c++",
                "meta.preprocessor.c++"
            ],
            "settings": {
                "foreground": "#FFB86C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Java Keywords",
            "scope": [
                "keyword.control.java",
                "storage.modifier.java",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#FF79C6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Java Types",
            "scope": [
                "storage.type.primitive.java",
                "storage.type.generic.java"
            ],
            "settings": {
                "foreground": "#8BE9FD",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Java Annotations",
            "scope": [
                "storage.type.annotation.java",
                "punctuation.definition.annotation.java"
            ],
            "settings": {
                "foreground": "#50FA7B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Assembly Instructions",
            "scope": [
                "keyword.operator.instruction",
                "entity.name.function.assembly"
            ],
            "settings": {
                "foreground": "#BD93F9",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Assembly Registers",
            "scope": "storage.register",
            "settings": {
                "foreground": "#FF79C6",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Hardware Addresses",
            "scope": [
                "constant.numeric.hex.assembly",
                "constant.numeric.binary"
            ],
            "settings": {
                "foreground": "#FFB86C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Memory Types",
            "scope": [
                "storage.type.memory",
                "storage.modifier.memory"
            ],
            "settings": {
                "foreground": "#8BE9FD",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Preprocessor Directives",
            "scope": [
                "meta.preprocessor",
                "keyword.control.directive"
            ],
            "settings": {
                "foreground": "#FF79C6",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Bitwise Operations",
            "scope": "keyword.operator.bitwise",
            "settings": {
                "foreground": "#FFB86C",
                "fontStyle": "bold"
            }
        }
    ],
