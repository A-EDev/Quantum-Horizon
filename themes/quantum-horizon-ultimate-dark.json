    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#424258",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"],
            "settings": {
                "foreground": "#7D5DEC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": ["entity.name.function", "meta.function-call", "meta.method-call"],
            "settings": {
                "foreground": "#50FA7B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes and Types",
            "scope": ["entity.name.type.class", "entity.name.class", "entity.name.type"],
            "settings": {
                "foreground": "#8BE9FD",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": ["variable", "variable.parameter"],
            "settings": {
                "foreground": "#E2E2E6"
            }
        },
        {
            "name": "Constants and Properties",
            "scope": ["variable.other.constant", "variable.other.property"],
            "settings": {
                "foreground": "#9590B3"
            }
        },
        {
            "name": "Strings and Templates",
            "scope": ["string", "string.template"],
            "settings": {
                "foreground": "#FF79C6"
            }
        },
        {
            "name": "Unit Tests",
            "scope": ["entity.name.function.test", "entity.name.function.test.js", "entity.name.function.test.ts"],
            "settings": {
                "foreground": "#50FA7B",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "GraphQL Operations",
            "scope": ["keyword.operation.graphql", "keyword.type.graphql"],
            "settings": {
                "foreground": "#FF79C6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "TOML Keys",
            "scope": "keyword.key.toml",
            "settings": {
                "foreground": "#8BE9FD"
            }
        },
        {
            "name": "Protocol Buffers",
            "scope": ["keyword.proto", "message.proto"],
            "settings": {
                "foreground": "#BD93F9",
                "fontStyle": "bold"
            }
        }
    ],
    "semanticTokenColors": {
        "namespace": "#7D5DEC",
        "class": "#8BE9FD",
        "enum": "#8BE9FD",
        "interface": "#8BE9FD",
