    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#404756",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": ["string", "string.quoted"],
            "settings": {
                "foreground": "#66CC99"
            }
        },
        {
            "name": "String Template Literals",
            "scope": ["string.template", "punctuation.definition.string.template"],
            "settings": {
                "foreground": "#99FFCC"
            }
        },
        {
            "name": "Number",
            "scope": ["constant.numeric", "constant.numeric.integer", "constant.numeric.float", "constant.numeric.hex", "constant.numeric.binary"],
            "settings": {
                "foreground": "#FF9966"
            }
        },
        {
            "name": "Built-in Constants",
            "scope": ["constant.language", "support.constant", "variable.language"],
            "settings": {
                "foreground": "#FDB813",
                "fontStyle": "bold"
            }
        },
        {
            "name": "User-defined Constants",
            "scope": ["constant.character", "constant.other"],
            "settings": {
                "foreground": "#FF9966"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#3399FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Control Flow Keywords",
            "scope": ["keyword.control", "keyword.operator.logical", "keyword.operator.comparison"],
            "settings": {
                "foreground": "#B380FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Function Declarations",
            "scope": ["entity.name.function", "support.function", "support.class"],
            "settings": {
                "foreground": "#FDB813",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Function Calls",
            "scope": ["meta.function-call", "variable.function"],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Class Declarations",
            "scope": ["entity.name.type.class", "entity.other.inherited-class", "support.class"],
            "settings": {
                "foreground": "#FF9966",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#E6E7E8"
            }
        },
        {
            "name": "Object Properties",
            "scope": ["variable.other.property", "variable.other.object.property", "support.variable.property"],
            "settings": {
                "foreground": "#66B3FF"
            }
        },
        {
            "name": "Parameters",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#FF9966",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tags",
            "scope": ["entity.name.tag", "punctuation.definition.tag"],
            "settings": {
                "foreground": "#3399FF"
            }
        },
        {
            "name": "Tag Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#FDB813",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": ["markup.heading", "entity.name.section"],
            "settings": {
                "foreground": "#FDB813",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": ["markup.underline.link", "string.other.link"],
            "settings": {
                "foreground": "#3399FF"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": ["markup.bold"],
            "settings": {
                "foreground": "#FFD700",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": ["markup.italic"],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Inline Code",
            "scope": ["markup.inline.raw", "markup.fenced_code"],
            "settings": {
                "foreground": "#66CC99"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#B380FF"
            }
        },
        {
            "name": "Escape Characters",
            "scope": ["constant.character.escape"],
            "settings": {
                "foreground": "#3399FF"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF3333"
            }
        },
        {
            "name": "Decorators",
            "scope": ["meta.decorator", "meta.decorator entity.name.function"],
            "settings": {
                "foreground": "#B380FF",
                "fontStyle": "italic"
            }
        }
    ]
}