{
    "name": "Quantum Horizon",
    "type": "dark",
    "colors": {
        "editor.background": "#0A1128",
        "editor.foreground": "#E6E6FA",
        "editorLineNumber.foreground": "#5D3FD3",
        "editorCursor.foreground": "#00FFD1",
        "sideBar.background": "#0F1630",
        "activityBar.background": "#0A1128",
        "statusBar.background": "#5D3FD3",
        "statusBar.foreground": "#FFFFFF",
        "tab.activeBackground": "#1A2345",
        "tab.inactiveBackground": "#0A1128",
        "panel.background": "#0F1630",
        "terminal.background": "#0A1128",
        "terminal.foreground": "#E6E6FA",
        "selection.background": "#5D3FD370"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#546E7A",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#C792EA",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#00FFD1"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#82AAFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#F78C6C"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#FFCB6B",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation.definition"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#f07178"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "entity.name.function.decorator"
            ],
            "settings": {
                "foreground": "#BB80B3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "Markdown Headings",
            "scope": [
                "markup.heading",
                "punctuation.definition.heading"
            ],
            "settings": {
                "foreground": "#82AAFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#00FFD1",
                "fontStyle": "underline"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable": "#00FFD1",
        "variable.readonly": "#82AAFF",
        "function": "#82AAFF",
        "method": "#82AAFF",
        "class": "#FFCB6B",
        "interface": "#C792EA",
        "enum": "#F78C6C",
        "enumMember": "#F78C6C",
        "parameter": "#89DDFF",
        "property": "#C3E88D"
    }
}