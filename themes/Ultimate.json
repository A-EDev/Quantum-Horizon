    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#5C6370",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": ["string"],
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
            "name": "Number",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#D19A66"
            }
        },
        {
            "name": "Keyword",
            "scope": ["keyword", "storage.type", "storage.modifier", "constant.language", "variable.language", "storage.type"],
            "settings": {
                "foreground": "#C678DD"
            }
        },
        {
            "name": "Operator",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#ABB2BF"
            }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation.accessor", "punctuation.definition", "punctuation.separator", "punctuation.terminator"],
            "settings": {
                "foreground": "#ABB2BF"
            }
        },
        {
            "name": "Function",
            "scope": ["entity.name.function", "meta.function-call", "support.function"],
            "settings": {
                "foreground": "#61AFEF"
            }
        },
        {
            "name": "Class",
            "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
            "settings": {
                "foreground": "#E5C07B"
            }
        },
        {
            "name": "Variable",
            "scope": ["variable", "support.variable", "variable.parameter"],
            "settings": {
                "foreground": "#ABB2BF"
            }
        },
        {
            "name": "Constant",
            "scope": ["constant", "support.constant"],
            "settings": {
                "foreground": "#D19A66"
            }
        },
        {
            "name": "Type",
            "scope": ["entity.name.type", "support.type"],
            "settings": {
              "foreground": "#87CEEB"
            }
        },
        {
            "name": "Tag",
            "scope": ["entity.name.tag", "punctuation.definition.tag"],
            "settings": {
                "foreground": "#E06C75"
            }
        },
        {
            "name": "Attribute",
            "scope": ["entity.other.attribute"],
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
          "name": "Invalid",
          "scope": "invalid",
          "settings": {
            "foreground": "#F44747",
            "fontStyle": "underline italic"
          }
        },
        {
        "name": "Function Parameter",
        "scope": ["variable.parameter"],
        "settings": {
            "foreground": "#ABB2BF",
            "fontStyle": "italic"
        }
    },
    {
        "name": "String Interpolation",
        "scope": ["punctuation.definition.template-expression", "meta.template.expression"],
        "settings": {
            "foreground": "#E06C75"
        }
    },
    {
        "name": "JSX/TSX Tags",
        "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx", "punctuation.definition.tag.jsx", "punctuation.definition.tag.tsx"],
        "settings": {
            "foreground": "#E06C75"
        }
    },
        {
        "name": "JSX/TSX Attributes",
        "scope": ["entity.other.attribute-name.jsx", "entity.other.attribute-name.tsx"],
        "settings": {
            "foreground": "#98C379",
            "fontStyle": "italic"
        }
    },
    {
        "name": "Markdown Headings",
        "scope": ["markup.heading", "punctuation.definition.heading.markdown"],
        "settings": {
            "foreground": "#61AFEF",
            "fontStyle": "bold"
        }
    },
        {
        "name": "Markup Bold",
        "scope": ["markup.bold"],
        "settings": {
            "fontStyle": "bold"
        }
    },
        {
        "name": "Markup Italic",
        "scope": ["markup.italic"],
        "settings": {
            "fontStyle": "italic"
        }
    },
        {
        "name": "Markup Deleted",
        "scope": "markup.deleted",
        "settings": {
          "foreground": "#808080",
          "fontStyle": "strikethrough"
        }
      },
      {
        "name": "Regular Expressions",
        "scope": "string.regexp",
        "settings": {
          "foreground": "#E06C75"
        }
      },
      {
        "name": "Type Annotations (TypeScript, etc.)",
        "scope": ["meta.type.annotation", "punctuation.separator.type"],
        "settings": {
            "foreground": "#87CEEB"
        }
    },
        {
            "name": "Special Method/Function",
            "scope": ["entity.name.function.member", "entity.name.function.method"],
            "settings": {
                "foreground": "#88C0D0",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Class Declaration",
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#EBCB8B",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Decorator",
            "scope": ["meta.decorator", "meta.decorator entity.name.function"],
            "settings": {
                "foreground": "#B48EAD",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Important Keywords",
            "scope": ["keyword.control.flow", "keyword.control.conditional"],
            "settings": {
                "foreground": "#81A1C1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Special Variables",
            "scope": ["variable.language.this", "variable.language.super"],
            "settings": {
                "foreground": "#D08770",
                "fontStyle": "italic bold"
            }
        }
    ]
}