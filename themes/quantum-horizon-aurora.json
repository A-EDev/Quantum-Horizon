    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#4D718C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "string constant.other.placeholder"],
            "settings": {
                "foreground": "#80FFEA"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#FF92DF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "meta.function-call"],
            "settings": {
                "foreground": "#8AFF80"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#FFFF80"
            }
        },
        {
            "name": "Decorators",
            "scope": ["meta.decorator", "meta.decorator.identifier"],
            "settings": {
                "foreground": "#FFFF80",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Properties",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#80FFEA"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": ["markup.heading", "entity.name.section.markdown"],
            "settings": {
                "foreground": "#FF92DF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": ["markup.underline.link", "string.other.link.title.markdown"],
            "settings": {
                "foreground": "#80FFEA",
                "fontStyle": "underline"
            }
        }
    ],
    "semanticTokenColors": {
        "namespace": "#FF92DF",
        "type": "#80FFEA",
        "class": "#80FFEA",
        "enum": "#80FFEA",
