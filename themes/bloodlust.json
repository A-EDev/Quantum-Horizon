    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#800000",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Blood Red Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Crimson Functions",
            "scope": ["entity.name.function", "meta.function-call"],
            "settings": {
                "foreground": "#FF3333",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Dark Red Classes",
            "scope": ["entity.name.type", "entity.name.class"],
            "settings": {
                "foreground": "#CC0000",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Blood Variables",
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#FFE6E6"
            }
        },
        {
            "name": "Special Blood Keywords",
            "scope": ["keyword.control.flow", "keyword.control.conditional"],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "String Literals",
            "scope": ["string"],
            "settings": {
                "foreground": "#FF6666"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": ["constant.numeric", "constant.language"],
            "settings": {
                "foreground": "#FF3333",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Function Parameters",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#FF9999",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Decorators",
            "scope": ["meta.decorator"],
            "settings": {
                "foreground": "#CC3333",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#FF3366"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "namespace": "#FF3333",
        "class": "#FF0000",
        "enum": "#CC0000",
        "interface": "#FF3333",
        "struct": "#CC0000",
        "typeParameter": "#FF6666",
        "parameter": "#FF9999",
        "variable": "#FFE6E6",
        "property": "#FF3333",
        "enumMember": "#FF0000",
        "event": "#FF3333",
        "function": "#FF0000",
        "method": "#FF3333",
        "macro": "#CC0000",
        "keyword": "#FF0000",
        "modifier": "#FF3333",
        "comment": "#800000",
        "string": "#FF6666",
        "number": "#FF3333",
        "regexp": "#FF3366",
        "operator": "#FF0000",
        "decorator": {
            "foreground": "#CC3333",
            "fontStyle": "italic bold"
        }
    }
}
