{
    "name": "Quantum Horizon (Bloodlust)",
    "type": "dark",
    "colors": {
        "editor.background": {
            "type": "gradient",
            "colors": ["#030101", "#050202", "#070303"],
            "stops": [0, 0.5, 1]
        },
        "editor.foreground": "#FFE6E6",
        "editorCursor.foreground": "#FF0000",
        "selection.background": "#800000",
        "editor.selectionBackground": "#80000088",
        "editor.selectionHighlightBackground": "#80000066",
        "editor.findMatchBackground": "#FF000044",
        "editor.findMatchHighlightBackground": "#FF000033",
        "editor.lineHighlightBackground": "#FF000015",
        "editor.lineHighlightBorder": "#FF000025",
        "editorRuler.foreground": "#400000",
        "editorBracketMatch.background": "#FF000033",
        "editorBracketMatch.border": "#FF0000",
        "editorError.foreground": "#FF0000",
        "editorWarning.foreground": "#FF3333",
        "editorInfo.foreground": "#CC0000",
        "editorHint.foreground": "#800000",
        "editorGutter.modifiedBackground": "#CC0000",
        "editorGutter.addedBackground": "#800000",
        "editorGutter.deletedBackground": "#FF0000",
        "editorOverviewRuler.border": "#400000",
        "editorOverviewRuler.errorForeground": "#FF0000",
        "editorOverviewRuler.warningForeground": "#FF3333",
        "editorOverviewRuler.infoForeground": "#CC0000",

        "activityBar.background": "#020000",
        "activityBar.foreground": "#FF0000",
        "activityBar.inactiveForeground": "#800000",
        "activityBarBadge.background": "#FF0000",
        "activityBarBadge.foreground": "#FFFFFF",
        "activityBar.border": "#400000",
        "activityBar.activeBorder": "#FF0000",

        "tab.activeBackground": "#200000",
        "tab.inactiveBackground": "#100000",
        "tab.activeForeground": "#FF0000",
        "tab.inactiveForeground": "#800000",
        "tab.activeBorderTop": "#FF0000",
        "tab.hoverBorder": "#FF0000",
        "tab.unfocusedActiveBorder": "#800000",

        "statusBar.background": "#020000",
        "statusBar.foreground": "#FF0000",
        "statusBar.border": "#400000",
        "statusBar.debuggingBackground": "#400000",
        "statusBar.debuggingForeground": "#FF0000",
        "statusBarItem.prominentBackground": "#800000",
        "statusBarItem.remoteBackground": "#FF0000",
        "statusBarItem.remoteForeground": "#FFFFFF",

        "panel.background": "#020000",
        "panel.border": "#400000",
        "panelTitle.activeBorder": "#FF0000",
        "panelTitle.activeForeground": "#FF0000",
        "panelTitle.inactiveForeground": "#800000",
        "sideBar.background": "#020000",
        "sideBar.foreground": "#FFE6E6",
        "sideBar.border": "#400000",
        "sideBarTitle.foreground": "#FF0000",
        "sideBarSectionHeader.background": "#100000",
        "sideBarSectionHeader.foreground": "#FF0000",
        "sideBarSectionHeader.border": "#400000",

        "list.activeSelectionBackground": "#400000",
        "list.activeSelectionForeground": "#FF0000",
        "list.inactiveSelectionBackground": "#200000",
        "list.inactiveSelectionForeground": "#FF3333",
        "list.hoverBackground": "#300000",
        "list.hoverForeground": "#FF0000",
        "list.highlightForeground": "#FF0000",
        "list.focusHighlightForeground": "#FF3333",
        "list.errorForeground": "#FF0000",
        "list.warningForeground": "#FF3333",

        "terminal.background": "#020000",
        "terminal.foreground": "#FFE6E6",
        "terminal.ansiBlack": "#200000",
        "terminal.ansiBrightBlack": "#400000",
        "terminal.ansiRed": "#FF0000",
        "terminal.ansiBrightRed": "#FF3333",
        "terminal.ansiGreen": "#800000",
        "terminal.ansiBrightGreen": "#CC0000",
        "terminal.ansiYellow": "#CC3300",
        "terminal.ansiBrightYellow": "#FF6600",
        "terminal.ansiBlue": "#990000",
        "terminal.ansiBrightBlue": "#CC0000",
        "terminal.ansiMagenta": "#CC0033",
        "terminal.ansiBrightMagenta": "#FF0066",
        "terminal.ansiCyan": "#800033",
        "terminal.ansiBrightCyan": "#CC0066",
        "terminal.ansiWhite": "#FFE6E6",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.selectionBackground": "#80000088",
        "terminalCursor.foreground": "#FF0000",
        "terminalCursor.background": "#FFFFFF"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#800000",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Blood Red Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Crimson Functions",
            "scope": ["entity.name.function", "meta.function-call"],
            "settings": {
                "foreground": "#FF3333",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Dark Red Classes",
            "scope": ["entity.name.type", "entity.name.class"],
            "settings": {
                "foreground": "#CC0000",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Blood Variables",
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#FFE6E6"
            }
        },
        {
            "name": "Special Blood Keywords",
            "scope": ["keyword.control.flow", "keyword.control.conditional"],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "String Literals",
            "scope": ["string"],
            "settings": {
                "foreground": "#FF6666"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": ["constant.numeric", "constant.language"],
            "settings": {
                "foreground": "#FF3333",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Function Parameters",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#FF9999",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Decorators",
            "scope": ["meta.decorator"],
            "settings": {
                "foreground": "#CC3333",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#FF3366"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "namespace": "#FF3333",
        "class": "#FF0000",
        "enum": "#CC0000",
        "interface": "#FF3333",
        "struct": "#CC0000",
        "typeParameter": "#FF6666",
        "parameter": "#FF9999",
        "variable": "#FFE6E6",
        "property": "#FF3333",
        "enumMember": "#FF0000",
        "event": "#FF3333",
        "function": "#FF0000",
        "method": "#FF3333",
        "macro": "#CC0000",
        "keyword": "#FF0000",
        "modifier": "#FF3333",
        "comment": "#800000",
        "string": "#FF6666",
        "number": "#FF3333",
        "regexp": "#FF3366",
        "operator": "#FF0000",
        "decorator": {
            "foreground": "#CC3333",
            "fontStyle": "italic bold"
        }
    }
}
