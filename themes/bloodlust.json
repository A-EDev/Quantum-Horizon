    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#801414",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Blood Red Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#FF1414",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Crimson Functions",
            "scope": ["entity.name.function", "meta.function-call"],
            "settings": {
                "foreground": "#FF4444",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Dark Red Classes",
            "scope": ["entity.name.type", "entity.name.class", "entity.name.namespace"],
            "settings": {
                "foreground": "#CC1414",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Blood Variables",
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#FFE6E6"
            }
        },
        {
            "name": "Special Blood Keywords",
            "scope": ["keyword.control.flow", "keyword.control.conditional"],
            "settings": {
                "foreground": "#FF1414",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "String Literals",
            "scope": ["string"],
            "settings": {
                "foreground": "#FF6666"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": ["constant.numeric", "constant.language"],
            "settings": {
                "foreground": "#FF4444",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Function Parameters",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#FF9999",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Decorators",
            "scope": ["meta.decorator"],
            "settings": {
                "foreground": "#CC3333",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#FF3366"
            }
        },
        {
            "name": "Imports and Exports",
            "scope": ["keyword.control.import", "keyword.control.export"],
            "settings": {
                "foreground": "#FF1414",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Object Properties",
            "scope": ["variable.other.property", "variable.other.object.property"],
            "settings": {
                "foreground": "#FF8080"
            }
        },
        {
            "name": "Tags",
            "scope": ["entity.name.tag", "punctuation.definition.tag"],
            "settings": {
                "foreground": "#FF1414",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#FF4444",
                "fontStyle": "italic"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "namespace": "#FF4444",
        "class": "#FF1414",
        "enum": "#CC1414",
        "interface": "#FF4444",
        "struct": "#CC1414",
        "typeParameter": "#FF6666",
        "parameter": "#FF9999",
        "variable": "#FFE6E6",
        "property": "#FF4444",
        "enumMember": "#FF1414",
        "event": "#FF4444",
        "function": "#FF1414",
        "method": "#FF4444",
        "macro": "#CC1414",
        "keyword": "#FF1414",
        "modifier": "#FF4444",
        "comment": "#801414",
        "string": "#FF6666",
        "number": "#FF4444",
        "regexp": "#FF3366",
        "operator": "#FF1414",
        "decorator": {
            "foreground": "#CC3333",
            "fontStyle": "italic bold"
        }
    }
}
