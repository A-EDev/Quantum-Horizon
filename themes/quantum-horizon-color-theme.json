{
    "name": "Quantum Horizon",
    "type": "dark",
    "colors": {
        "editor.background": "#0D0E1B",
        "editor.foreground": "#EAEAEA",
        "editorCursor.foreground": "#FFD700",
        "editor.lineHighlightBackground": "#1A1B2D",
        "editorLineNumber.foreground": "#B0B0B0",
        "editor.selectionBackground": "#3A3A3A",
        "editor.selectionHighlightBackground": "#3A3A3A",
        "editor.wordHighlightBackground": "#3A3A3A",
        "editor.wordHighlightStrongBackground": "#3A3A3A",
        "editorBracketMatch.background": "#1A1B2D",
        "editorBracketMatch.border": "#FFD700",
        "editorError.foreground": "#FF4C4C",
        "editorWarning.foreground": "#FFA500",
        "editorInfo.foreground": "#50C878",
        "activityBar.background": "#1A1B2D",
        "activityBar.foreground": "#EAEAEA",
        "activityBar.activeBorder": "#FFD700",
        "statusBar.background": "#1A1B2D",
        "statusBar.foreground": "#EAEAEA",
        "sideBar.background": "#1A1B2D",
        "sideBar.foreground": "#B0B0B0",
        "editorGroupHeader.tabsBackground": "#1A1B2D",
        "editorGroup.border": "#3A3A3A",
        "titleBar.activeBackground": "#0D0E1B",
        "titleBar.activeForeground": "#EAEAEA",
        "titleBar.inactiveBackground": "#1A1B2D",
        "titleBar.inactiveForeground": "#B0B0B0",
        "panel.background": "#1A1B2D",
        "panel.border": "#3A3A3A"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#6A9955",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.operator",
                "keyword.control"
            ],
            "settings": {
                "foreground": "#FFD700",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#50C878"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.interpolated"
            ],
            "settings": {
                "foreground": "#9966CC"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "variable.function"
            ],
            "settings": {
                "foreground": "#0F52BA",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#50C878"
            }
        },
        {
            "scope": [
                "meta.embedded"
            ],
            "settings": {
                "foreground": "#50C878"
            }
        },
        {
            "scope": [
                "support.class",
                "support.function"
            ],
            "settings": {
                "foreground": "#0F52BA"
            }
        },
        {
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#FFD700",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "meta.function-call",
                "meta.function-call.arguments"
            ],
            "settings": {
                "foreground": "#0F52BA"
            }
        },
        {
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#FF4C4C"
            }
        },
        {
            "scope": [
                "variable.other.local"
            ],
            "settings": {
                "foreground": "#FFD700",
                "fontStyle": "italic"
            }
        }
    ],
    "semanticHighlighting": true
}